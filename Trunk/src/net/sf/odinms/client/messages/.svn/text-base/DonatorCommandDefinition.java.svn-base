package net.sf.odinms.client.messages;

/**
 *
 * @author Admin
 */
public class DonatorCommandDefinition {

    private String donatorcommand;
    private String parameterDescription;
    private String help;

    public DonatorCommandDefinition(String command, String parameterDescription, String help) {
        this.donatorcommand = command;
        this.help = help;
        this.parameterDescription = parameterDescription;
    }

    public String getCommand() {
        return donatorcommand;
    }

    public String getHelp() {
        return help;
    }

    public String getParameterDescription() {
        return parameterDescription;
    }
}
